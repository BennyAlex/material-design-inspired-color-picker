{
  "name": "material-design-inspired-color-picker",
  "version": "2.0.0",
  "description": "A color picker inspired by material design. It is highly customizable and supports custom colors and palettes.",
  "main": "dist/md-color-picker.js",
  "scripts": {
    "build": "rimraf dist/md-color-picker.js && rimraf docs/md-color-picker.js && cross-env NODE_ENV=production rollup --config --compact",
    "dev": "rimraf docs/md-color-picker.js && cross-env NODE_ENV=develop rollup --config --watch"
  },
